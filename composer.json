{
    "name": "yajra/laravel-address",
    "description": "Philippines Address Lookup API for Laravel.",
    "keywords": [
        "yajra",
        "laravel",
        "address"
    ],
    "homepage": "https://github.com/yajra/laravel-address",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com",
            "homepage": "http://yajrabox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/database": "^10.0",
        "laravelcollective/html": "^6.4.1",
        "rap2hpoutre/fast-excel": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^10",
        "larastan/larastan": "^2.9",
        "rector/rector": "^1.0",
        "orchestra/testbench": "^8.21",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
      "classmap": [
        "database"
      ],
      "psr-4": {
            "Yajra\\Address\\": "src",
            "Yajra\\Address\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\Address\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\Address\\AddressServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
